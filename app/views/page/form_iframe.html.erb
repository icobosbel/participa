<% content_for :title, title %>
<div class="content-content cols">
  <div class="row">
    <div class="col-b-4a12">
      <%= content_tag :h2, title %>
      <% if defined? return_path %>
        <%= content_tag :p, link_to("Volver", return_path) %>
      <% end %>
      <iframe id="js-iframe" src="<%=url%>" width="100%" height="500" frameBorder="0" class="gfiframe"></iframe>
      <script src="//<%= Rails.application.secrets.forms["domain"] %>/wp-content/plugins/gravity-forms-iframe/assets/scripts/gfembed.min.js" type="text/javascript"></script>
    </div>
  </div>
</div>
